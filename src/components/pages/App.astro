---
import Layout from "../../layouts/Layout.astro";
import LastRelease from "../LastRelease.astro";

import { getI18N } from "../../i18n/";

const { currentLocale } = Astro;
const i18n = getI18N({ currentLocale });
---

<Layout
  title=`${i18n.SEO_TITLE_1} - ${i18n.SEO_TITLE_2}`
  description={i18n.SEO_DESCRIPTION}
>
  <link
    rel="preload"
    href="/fonts/jura.woff2"
    as="font"
    type="font/woff2"
    crossorigin
    slot="preload"
  />

  <main>
    <LastRelease />
  </main>
</Layout>
